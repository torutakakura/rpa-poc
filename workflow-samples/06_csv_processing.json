{
  "name": "CSV処理",
  "description": "CSVファイルの読み書き処理",
  "version": "1.0.0",
  "steps": [
    {
      "id": "step-001",
      "category": "K_CSV",
      "operation": "CSV書き込み",
      "params": {
        "file_path": "C:/temp/data.csv",
        "data": [
          ["ID", "名前", "年齢", "部署"],
          ["001", "田中太郎", "30", "営業部"],
          ["002", "鈴木花子", "25", "開発部"],
          ["003", "佐藤次郎", "35", "総務部"]
        ]
      },
      "description": "CSVファイルにデータを書き込み"
    },
    {
      "id": "step-002",
      "category": "B_待機・終了・エラー",
      "operation": "待機",
      "params": {
        "seconds": 1
      },
      "description": "1秒待機"
    },
    {
      "id": "step-003",
      "category": "K_CSV",
      "operation": "CSV読み込み",
      "params": {
        "file_path": "C:/temp/data.csv"
      },
      "description": "CSVファイルを読み込み"
    },
    {
      "id": "step-004",
      "category": "E_記憶",
      "operation": "変数に保存",
      "params": {
        "variable_name": "csv_data",
        "value": "${csv_content}"
      },
      "description": "読み込んだデータを変数に保存"
    },
    {
      "id": "step-005",
      "category": "K_CSV",
      "operation": "CSV追記",
      "params": {
        "file_path": "C:/temp/data.csv",
        "data": [
          ["004", "山田三郎", "28", "営業部"],
          ["005", "高橋四郎", "32", "開発部"]
        ]
      },
      "description": "CSVファイルに追記"
    }
  ]
}